{
    "name": "qm-fe-cli",
    "version": "1.0.4",
    "description": "front-end cli",
    "author": "shenmeng@qimao.com",
    "bin": {
        "fe-cli": "./bin/run"
    },
    "homepage": "https://github.com/code/fe-cli",
    "license": "MIT",
    "main": "dist/index.js",
    "repository": "code/fe-cli",
    "files": [
        "/bin",
        "/dist",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "dependencies": {
        "@inquirer/prompts": "^3.2.0",
        "@oclif/core": "^3",
        "@oclif/plugin-help": "^5",
        "@oclif/plugin-plugins": "^3.9.1",
        "eslint-config-prettier": "^9.0.0",
        "fs-extra": "^11.1.1",
        "ora": "^5.4.1"
    },
    "devDependencies": {
        "@oclif/prettier-config": "^0.2.1",
        "@oclif/test": "^3",
        "@q-front-npm-configs/eslint": "1.0.4",
        "@types/chai": "^4",
        "@types/mocha": "^9.0.0",
        "@types/node": "^18",
        "@typescript-eslint/eslint-plugin": "^6.9.0",
        "@typescript-eslint/parser": "^6.9.0",
        "chai": "^4",
        "chalk": "^4.1.2",
        "download-git-repo": "^3.0.2",
        "eslint": "^8.51.0",
        "eslint-config-oclif": "^5",
        "eslint-config-oclif-typescript": "^3",
        "mocha": "^10",
        "oclif": "^4.0.3",
        "prettier": "^2.5.1",
        "shx": "^0.3.3",
        "ts-node": "^10.9.1",
        "typescript": "^5"
    },
    "oclif": {
        "bin": "fe-cli",
        "dirname": "fe-cli",
        "commands": "./dist/commands",
        "plugins": [
            "@oclif/plugin-help",
            "@oclif/plugin-plugins"
        ],
        "topicSeparator": " ",
        "topics": {
            "hello": {
                "description": "Say hello to the world and others"
            }
        }
    },
    "scripts": {
        "build": " shx rm -rf dist  && tsc -b -w && cp -r ./templates ./dist/templates",
        "lint": "eslint . --ext .ts",
        "postpack": "shx rm -f oclif.manifest.json",
        "posttest": "npm run lint",
        "test": "mocha --forbid-only \"test/**/*.test.ts\"",
        "version": "oclif readme && git add README.md",
        "prettier": "prettier --write 'src/**/*.ts'"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "bugs": "https://github.com/code/fe-cli/issues",
    "keywords": [
        "oclif"
    ],
    "types": "dist/index.d.ts"
}
